{
    "name": "rapideinternet/bakku-client",
    "version": "1.0.0",
    "description": "Bakku Client for interacting with the Bakku API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max van der Werf",
            "email": "max.vanderwerf@rapide.software"
        }, {
            "name": "Rapide Software",
            "email": "info@rapide.software"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "RapideSoftware\\BakkuClient\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RapideSoftware\\BakkuClient\\BakkuClientServiceProvider"
            ],
            "aliases": {
                "BakkuClient": "RapideSoftware\\BakkuClient\\Facades\\BakkuClient"
            }
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^8.0"
    },
    "scripts": {
        "phpstan": "phpstan analyse --level=max src/"
    }
}
